                    <div class="col-md-8" style="border-style:none;">
                        <pre class="brush: js">
                        <h4>Factorials!</h4>
                        <p>Enter a number and click the button to find the factorial of that number.</p>
                        <input id="x" type="number" placeholder="Enter A Number" />

                        <p>&nbsp;</p>
                        <button onclick="factorial()" class="btn btn-sm btn-block btn-custom-1">Find the Factorial</button>
                        <p>&nbsp;</p>
                        <p id="FactResult"></p>

            function factorial() {
            var x = Number(document.getElementById("x").value);

            if (x == 0) {
                result = 1
                document.getElementById("FactResult").innerHTML = result;
                return;
            }
            var result = x;
            for (var i = x - 1; i >= 1; i--) {
                result = result * i;

            }
            document.getElementById("FactResult").innerHTML = result;
        }
