            }

            var Number10 = Number((Number1 + Number2 + Number3 + Number4 + Number5) / 5);
            document.getElementById("result5").innerHTML = Number10 + " is the mean of these numbers!";
        }


        function factorial() {
            var x = Number(document.getElementById("x").value);

            if (x == 0) {
                result = 1
                document.getElementById("FactResult").innerHTML = result;
                return;
            }
            var result = x;
            for (var i = x - 1; i >= 1; i--) {
                result = result * i;

            }
            document.getElementById("FactResult").innerHTML = result;
        }

        function Iteration() {
            var inputVal1 = document.getElementById("iterate").value;
            var inputVal2 = document.getElementById("iterate2").value;
            var result = ""

            if (inputVal1 == "" || inputVal2 == "" || inputVal1 > 100 || inputVal2 > 100) {
                alert("Please enter a number between 1 and 100 into each field.");
                return;
            }

            for (var i = 1; i <= 100; i++) {

                if (i % inputVal1 == 0 && i % inputVal2 == 0) {
                    result = result + " fizzbuzz" + "<br>"
                }

                else if (i % inputVal1 == 0) {
                    result = result + " fizz" + " "
                }
                else if (i % inputVal2 == 0) {
                    result = result + " buzz" + "<br>"
                }


                else {
                    result = result + i + " ";
                }
            }
            document.getElementById("fizzbuzz").innerHTML = result
        }

        function Palindrome() {
            var string = document.getElementById("mytext").value;
            var remove = /[\W_0-9]/g;
            var regulartext = string.toLowerCase().replace(remove, "");
            var reversetext = regulartext.split("").reverse().join("");

            if (!isNaN(regulartext) || regulartext == ("~!@@#$%^&*()_+=-[]{}/?<,>.") || regulartext == "") {
                alert("Please enter a word or string of letters into the field.");
                return;
            }

            else if (reversetext == regulartext) {
                document.getElementById("PalResult").innerHTML = regulartext + " is a Palindrome.";
            }
            else {
                document.getElementById("PalResult").innerHTML = regulartext + " is not a Palandrome.";
            }

        }

