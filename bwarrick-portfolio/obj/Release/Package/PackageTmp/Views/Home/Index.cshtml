                    <div class="col-md-8" style="border-style:none;">
                        <pre class="brush: js">
                        <h4>Fizz! Buzz!</h4>
                        <p>
             Enter two numbers between one and one hundred. When the button is clicked,
             you will see a list from one to one hundred with each multiple of your first
             number listed as "fizz" and each multiple of your second number listed as "buzz."
             Multiples of both numbers will be listed as "fizzbuzz."
            </p>
                        <input type="number" id="iterate" placeholder="Enter Number 1" />

                        <p>&nbsp;</p>
                        <input type="number" id="iterate2" placeholder="Enter Number 2" />

                        <p>&nbsp;</p>
                        <button onclick="Iteration()" class="btn btn-sm btn-block btn-custom-1">Click Me!</button>
                        <div id="fizzbuzz"></div>
                        <p>&nbsp;</p>

            function Iteration() {
            var inputVal1 = document.getElementById("iterate").value;
            var inputVal2 = document.getElementById("iterate2").value;
            var result = ""

            if (inputVal1 == "" || inputVal2 == "" || inputVal1 > 100 || inputVal2 > 100) {
                alert("Please enter a number between 1 and 100 into each field.");
                return;
            }

            for (var i = 1; i <= 100; i++) {

                if (i % inputVal1 == 0 && i % inputVal2 == 0) {
                    result = result + " fizzbuzz" + "<br>"
                }

                else if (i % inputVal1 == 0) {
                    result = result + " fizz" + " "
                }
                else if (i % inputVal2 == 0) {
                    result = result + " buzz" + "<br>"
                }


                else {
                    result = result + i + " ";
                }
            }
            document.getElementById("fizzbuzz").innerHTML = result
        }
                        </pre>
                    </div>
                </div>
            </div>
