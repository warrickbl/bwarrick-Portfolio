                    <div class="modal-title" style="border-style:none;">
                        <h2>PALINDROMES</h2>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4" style="border-style:none;">
                        <h4>Palindrome Identifier</h4>
                        <p>Enter a word or string of letters, then click the button to find out whether or not it is a Palindrome!</p>
                        <input type="text" id="mytext" placeholder="Enter a word or sentence." />
                        <p>&nbsp;</p>
                        <button onclick="Palindrome()" class="btn btn-sm btn-block btn-custom-1">Is this a Palindrome?</button>
                        <p>&nbsp;</p>
                        <div id="PalResult"></div>
                        <p>&nbsp;</p>
                    </div>
                    <div class="col-md-8" style="border-style:none;">
                        <pre class="brush: js">

                        <h4>Palindrome Identifier</h4>
                        <p>Enter a word or string of letters, then click the button to find out whether or not it is a Palindrome!</p>
                        <input type="text" id="mytext" placeholder="Enter a word or sentence." />

                        <p>&nbsp;</p>
                        <button onclick="Palindrome()" class="btn btn-sm btn-block btn-custom-1">Is this a Palindrome?</button>
                        <p>&nbsp;</p>
                        <div id="PalResult"></div>
                        <p>&nbsp;</p>


            function Palindrome() {
            var string = document.getElementById("mytext").value;
            var remove = /[\W_0-9]/g;
            var regulartext = string.toLowerCase().replace(remove, "");
            var reversetext = regulartext.split("").reverse().join("");

            if (!isNaN(regulartext) || regulartext == ("~!@@#$%^&*()_+=-[]{}/?<,>.") || regulartext == "") {
                alert("Please enter a word or string of letters into the field.");
                return;
            }

            else if (reversetext == regulartext) {
                document.getElementById("PalResult").innerHTML = regulartext + " is a Palindrome.";
            }
            else {
                document.getElementById("PalResult").innerHTML = regulartext + " is not a Palandrome.";
            }

        }
                        </pre>
                    </div>
                </div>
            </div>
        </div>
